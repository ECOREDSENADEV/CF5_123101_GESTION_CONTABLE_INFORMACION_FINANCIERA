array) {\n      let currentIndex = array.length\n      let randomIndex\n      while (currentIndex > 0) {\n        randomIndex = Math.floor(Math.random() * currentIndex)\n        currentIndex--\n        ;[array[currentIndex], array[randomIndex]] = [\n          array[randomIndex],\n          array[currentIndex],\n        ]\n      }\n      return array\n    },*/\n    onRrespuestaSelected(respuestaEsCorrecta) {\n      this.continuarDisabled = false\n      this.respuestaActual = {\n        id: this.preguntaSelected.id,\n        esCorrecta: respuestaEsCorrecta,\n      }\n      if (respuestaEsCorrecta) {\n        this.reproducirSonido(successSound)\n      } else {\n        this.reproducirSonido(failSound)\n      }\n    },\n    onContinuar() {\n      this.continuarDisabled = true\n      if (this.respuestaActual.id) {\n        const idx = this.respuestas.findIndex(\n          r => r.id === this.preguntaSelected.id,\n        )\n        if (idx === -1) {\n          this.respuestas.push(this.respuestaActual)\n        } else {\n          this.respuestas[idx] = this.respuestaActual\n        }\n      }\n\n      if (this.preguntaSelectedIdx < this.preguntas.length - 1) {\n        this.preguntaSelectedIdx += 1\n        this.reproducirSonido(screenChangeSound)\n      } else {\n        this.finalizarPrueba()\n      }\n    },\n    onReiniciar() {\n      this.preguntaSelectedIdx = 0\n      this.respuestas = []\n      this.respuestaActual = {}\n      this.intentos += 1\n      this.$emit('reiniciar')\n    },\n    reproducirSonido(audioSrc) {\n      if (this.audioEnabled) {\n        const audio = new Audio(audioSrc)\n        audio.play()\n      }\n    },\n    finalizarPrueba() {\n      const totalPreguntas = this.preguntas.length\n      const respuestasCorrectas = this.respuestas.filter(r => r.esCorrecta)\n        .length\n      const porcentajeAprobacion = (respuestasCorrectas / totalPreguntas) * 100\n\n      if (porcentajeAprobacion >= 70) {\n        this.reproducirSonido(endGameSuccessSound)\n      } else {\n        this.reproducirSonido(endGameFailSound)\n      }\n    },\n  },\n}\n</script>\n\n<style lang=\"sass\" scoped>\n.boton--disabled\n  opacity: 0.5\n  pointer-events: none\n\n.tarjeta--lightest-gray\n  border: 3px solid #dce4eb\n</style>\n"],"mappings":";;;;AA6DA,OAAAA,iBAAA;AACA,OAAAC,YAAA;AACA,OAAAC,SAAA;AACA,OAAAC,mBAAA;AACA,OAAAC,gBAAA;AACA,OAAAC,iBAAA;AACA,OAAAC,oBAAA;AACA,OAAAC,mBAAA;AACA;EACAC,IAAA;EACAC,UAAA;IACAJ,iBAAA;IACAC,oBAAA;IACAC;EACA;EACAG,KAAA;IACAC,YAAA;MACAC,IAAA,EAAAC,MAAA;MACAC,QAAA;IACA;IACAC,iBAAA;MACAH,IAAA,EAAAI,OAAA;MACAC,OAAA;IACA;EACA;EACAC,IAAA,EAAAA,CAAA;IACAC,QAAA;IACAC,mBAAA;IACAC,eAAA;IACAC,UAAA;IACAC,iBAAA;IACAC,YAAA;IACAC,wBAAA;EACA;EACAC,QAAA;IACAC,UAAA;MACA,MAAAC,aAAA;MACA;QAAAD;MAAA,SAAAhB,YAAA;MACA,KAAAgB,SAAA;MAEA,MAAAE,kBAAA,QAAAC,OAAA,CAAAH,SAAA;MAEA,MAAAI,sBAAA,GACAF,kBAAA,CAAAG,MAAA,IAAAJ,aAAA,GACAC,kBAAA,CAAAI,KAAA,IAAAL,aAAA,IACAC,kBAAA;MAEAK,OAAA,CAAAC,GAAA,CACA,0BACAJ,sBAAA,EACA,sBACAF,kBACA;MAEA,OAAAE,sBAAA,CAAAK,GAAA,CAAAC,QAAA;QACA,GAAAA,QAAA;QACAC,QAAA,EAAAD,QAAA,CAAAE,iBAAA,GACA,KAAAT,OAAA,CAAAO,QAAA,CAAAC,QAAA,IACAD,QAAA,CAAAC,QAAA;QACAnB,QAAA,OAAAA;MACA;IACA;IACAqB,iBAAA;MACA,YAAAb,SAAA,MAAAP,mBAAA;IACA;IACAqB,oBAAA;MACA,SAAAnB,UAAA,CAAAU,MAAA;MACA,MAAAU,SAAA,QAAApB,UAAA,CAAAqB,MAAA,CAAAC,CAAA,IAAAA,CAAA,CAAAC,UAAA,EAAAb,MAAA;MACA,OAAAc,IAAA,CAAAC,KAAA,CAAAL,SAAA,QAAApB,UAAA,CAAAU,MAAA;IACA;EACA;EACAgB,OAAA;IACAlB,QAAAmB,KAAA;MACA,SAAAC,CAAA,GAAAD,KAAA,CAAAjB,MAAA,MAAAkB,CAAA,MAAAA,CAAA;QACA,MAAAC,CAAA,GAAAL,IAAA,CAAAM,KAAA,CAAAN,IAAA,CAAAO,MAAA,MAAAH,CAAA;QACA,CAAAD,KAAA,CAAAC,CAAA,GAAAD,KAAA,CAAAE,CAAA,MAAAF,KAAA,CAAAE,CAAA,GAAAF,KAAA,CAAAC,CAAA;MACA;MACA,OAAAD,KAAA;IACA;IACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;IACAK,qBAAAC,mBAAA;MACA,KAAAhC,iBAAA;MACA,KAAAF,eAAA;QACAmC,EAAA,OAAAhB,gBAAA,CAAAgB,EAAA;QACAX,UAAA,EAAAU;MACA;MACA,IAAAA,mBAAA;QACA,KAAAE,gBAAA,CAAAxD,YAAA;MACA;QACA,KAAAwD,gBAAA,CAAAvD,SAAA;MACA;IACA;IACAwD,YAAA;MACA,KAAAnC,iBAAA;MACA,SAAAF,eAAA,CAAA